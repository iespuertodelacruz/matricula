@font-face
    font-family: "BebasNeue"
    src: url("../fonts/BebasNeue.otf")

h1, h2, h3, h4, h5, h6
    font-family: "BebasNeue"

.navbar .row
    padding-top: 10px
    padding-bottom: 10px
    img
        padding-top: 7px
    a
        color: #333333

for i in (1..10)
    #div_id_core_subject_order{i}
        background-color: rgba(250, 246, 215, 0.63)
        border: 1px solid darken(rgba(250, 246, 215, 0.63), 30%)
    #div_id_specific_subject1_order{i}
        background-color: rgba(250, 230, 237, 0.63)
        border: 1px solid darken(rgba(250, 230, 237, 0.63), 30%)
    #div_id_specific_subject2_order{i}
        background-color: rgba(230, 250, 236, 0.63)
        border: 1px solid darken(rgba(230, 250, 236, 0.63), 30%)
    #div_id_specific_subject3_order{i}
        background-color: rgba(230, 240, 250, 0.63)
        border: 1px solid darken(rgba(230, 240, 250, 0.63), 30%)

.error
    color: #a94442
    font-weight: bold
